{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "max-len": ["error", { "code": 150 }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".tsx"] }],
    "linebreak-style": "off",
    "import/prefer-default-export": "off"
  }
}
